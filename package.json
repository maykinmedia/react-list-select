{
  "name": "react-list-select",
  "version": "0.4.0",
  "description": "List with selectable and navigable items",
  "keywords": [
    "react-component"
  ],
  "main": "dist/react-list-select.js",
  "style": "index.css",
  "files": [
    "index.js",
    "keys.js",
    "List.js",
    "ListItem.js",
    "README.md",
    "CHANGELOG.md"
  ],
  "author": "Navaru",
  "contributors": [
    "Hawken Rives <hawkrives@gmail.com>"
  ],
  "license": "ISC",
  "repository": "hawkrives/react-list-select",
  "devDependencies": {
    "babel-preset-react": "^6.24.1",
    "babelify": "^5.0.3",
    "browser-sync": "^2.2.1",
    "browserify": "^9.0.3",
    "exorcist": "^0.1.6",
    "gh-pages-deploy": "^0.3.0",
    "react": "^0.12.2",
    "watchify": "^2.4.0"
  },
  "dependencies": {
    "classnames": "^1.1.4",
    "lodash": "^3.3.1"
  },
  "config": {
    "browserify": "--debug -t [babelify] -e index.es6 --extension=.es6"
  },
  "scripts": {
    "clean": "rm -f dist/*.js example/*.js",
    "make": "babel ./*.es6 --out-dir dist/ && browserify $npm_package_config_browserify -e dist/index.js > dist/react-list-select.js",
    "make-example": "browserify $npm_package_config_browserify -e example/index.es6 > example/bundle.js",
    "build": "npm run make",
    "start": "npm run browser-sync & npm run watch; wait",
    "prepare": "npm run build",
    "postpublish": "npm run clean",
    "watch": "watchify -v $npm_package_config_browserify -e example/index.es6 -o example/bundle.js",
    "browser-sync": "browser-sync start --files='example/*.js, example/*css, example/*.html}' --server=example/",
    "deploy": "gh-pages-deploy",
    "post-deploy": "npm run clean"
  },
  "gh-pages-deploy": {
    "staticpath": "example",
    "prep": [
      "make-example"
    ],
    "noprompt": true
  }
}
